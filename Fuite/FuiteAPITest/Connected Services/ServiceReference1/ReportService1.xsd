      <xs:element minOccurs="0" name="id" type="xs:int" />
      <xs:element minOccurs="0" name="state" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Change" nillable="true" type="tns:Change" />
  <xs:complexType name="ResultIp">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:Result">
        <xs:sequence>
          <xs:element minOccurs="0" name="Data" type="xs:boolean" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ResultIp" nillable="true" type="tns:ResultIp" />
</xs:schema>